{
  "name": "mon-app-js",
  "version": "1.0.0",
  "description": "Application JavaScript pour TP Jenkins",
  "main": "src/app.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "build": "rimraf dist && mkdir dist && xcopy src dist /E /I /Y",
    "clean": "rimraf dist node_modules"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "junit.xml"
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^5.0.10"
  },
  "dependencies": {
    "express": "^4.18.0"
  }
}
